<resources>
  <declare-styleable name="whsCheckBox">
    <attr name="checkBoxAtRight" format="boolean" defaultValue="false" />
    <attr name="text" format="string"/>
    <attr name="summary" format="string" />
    <attr name="summaryOn" format="string" />
    <attr name="summaryOff" format="string" />
    <attr name="checked" format="boolean"/>
  </declare-styleable>
  <declare-styleable name="whsInstalledApplicationsList">
      <attr name="title" format="string"/>
  </declare-styleable>
  <declare-styleable name="whsPreferencesView">
        <attr name="exampleString" format="string" />
        <attr name="exampleDimension" format="dimension" />
        <attr name="exampleColor" format="color" />
        <attr name="exampleDrawable" format="color|reference" />
    </declare-styleable>
  
</resources>